<openerp>
  <data>
    <!-- explicit school view definition -->

    <record model="ir.ui.view" id="school.school">
      <field name="name">school</field>
      <field name="model">school.school</field>
      <field name="arch" type="xml">


         <tree>

            <field name="x_city"/>
            <field name="x_town"/>
            <field name="x_schoolName"/>
            <field name="x_equipmentNumber"/>
            <field name="x_schoolAddress"/>
            <field name="x_Name"/>
            <field name="x_contrctPhoneNumber"/>
            <field name="x_checkCondition"/>
            <field name="x_checkCertificationNumber"/>

            <field name="x_receptionData"/>
            <field name="x_trainingteacher"/>
            <field name="x_phoneNumber"/>
            <field name="x_contact2"/>
            <field name="x_phoneNumber2"/>
            <field name="x_QQ"/>
            <field name="x_mail"/>
            <field name="x_remark"/>

        </tree>


          <form string="Session Form">
                    <sheet>
                        <group>
                            <group string="General">
                                 <field name="x_city"/>
                                <field name="x_town"/>
                                <field name="x_schoolName"/>
                                <field name="x_equipmentNumber"/>
                                <field name="x_schoolAddress"/>
                                <field name="x_Name"/>
                                <field name="x_contrctPhoneNumber"/>
                                <field name="x_checkCondition"/>
                                <field name="x_checkCertificationNumber"/>
                            </group>
                            <group string="Schedule">
                                <field name="x_receptionData"/>
                               <field name="x_trainingteacher"/>
                               <field name="x_phoneNumber"/>
                               <field name="x_contact2"/>
                               <field name="x_phoneNumber2"/>
                               <field name="x_QQ"/>
                               <field name="x_mail"/>
                              <field name="x_remark"/>
                            </group>
                        </group>
                    </sheet>
          </form>



      </field>
    </record>



